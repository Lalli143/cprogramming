#include<stdio.h>
int main()
{
int num,i,evensum=0,oddsum=0;
printf("Enter a number:");
scanf("%d",&num);
printf("Numbers divisible by %d:\n",num);
for(i=1;i<=20;i++)
{
if(i% num ==0)
{
Printf("%d\n",i);
if(i%2==0)
{
evensum+=i;
}
else
{
Oddsum+=i;
   }
  }
 }
printf("evensum=%d\n",evensum);
printf("oddsum=%d\n", oddsum);
return 0;
}
